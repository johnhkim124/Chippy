;\n  const [isLoaded, setLoaded] = useState(false);\n  const [refresh, setRefresh] = useState(false);\n  const { id } = useParams();\n  const history = useHistory();\n  const { handleAddToCart } = props;\n\n  useEffect(() => {\n    const fetchSnack = async () => {\n      const snack = await getSnack(id);\n      setSnack(snack);\n      setLoaded(true);\n    };\n    fetchSnack();\n  }, [id, refresh]);\n\n  if (!isLoaded) {\n    return <h1>Loading...</h1>;\n  }\n\n  return (\n    <div className=\"snack-detail-div\">\n      {snack && (\n        <>\n          <div className=\"snack-img-div\">\n            <img src={snack.img_url} alt={snack.name} />\n          </div>\n          <div className=\"snack-info\">\n            <h1>{snack.name}</h1>\n            <p>Category: {snack.category}</p>\n            <p>{snack.origin}</p>\n            <h3>Prices: ${snack.price}</h3>\n\n            <button\n              className=\"add-to-cart\"\n              onClick={() => {\n                handleAddToCart(snack.id);\n              }}\n            >\n              Add to Cart\n            </button>\n            <Link to={`/${snack.id}/edit`}>\n              <button>Edit</button>\n            </Link>\n\n            <button\n              onClick={async () => {\n                await deleteSnack(snack.id);\n                history.push(\"/snacks\");\n                setRefresh(!refresh);\n              }}\n            >\n              Delete\n            </button>\n          </div>\n        </>\n      )}\n    </div>\n  );\n};\n\nexport default SnackDetail;\n","/home/johnkim/general-assembly/unit-4/Chippy/Chippy/chippy-client/src/screens/editItem/EditItem.jsx",[],"/home/johnkim/general-assembly/unit-4/Chippy/Chippy/chippy-client/src/components/singleSnack/SingleSnack.jsx",[],"/home/johnkim/general-assembly/unit-4/Chippy/Chippy/chippy-client/src/sevices/cart.js",[],"/home/johnkim/general-assembly/unit-4/Chippy/Chippy/chippy-client/src/screens/cart/Cart.jsx",[],"/home/johnkim/general-asse